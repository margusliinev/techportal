        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^6.7.0",
        "googleapis": "^120.0.0",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.0",
        "nodemailer": "^6.9.3",
        "pg": "^8.11.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
